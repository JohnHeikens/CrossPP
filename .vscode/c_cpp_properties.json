{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${default}",
                "${workspaceFolder}/**",
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks/Kernel.framework/Versions/A/Headers",
                "${workspaceFolder}/build/_deps/sfml-src/include"
            ],
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "intelliSenseMode": "macos-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Win32",
            "includePath": [
                "${default}",
                "${workspaceFolder}/**",
                "C:/msys64/mingw64/include/c++/13.2.0",
                "C:/msys64/mingw64/include/c++/13.2.0/x86_64-w64-mingw32",
                "C:/msys64/mingw64/lib/gcc/x86_64-w64-mingw32/13.2.0/include",
                "${workspaceFolder}/build/_deps/sfml-src/include"
            ],
            "compilerPath": "C:/msys64/mingw64/bin/g++.exe",
            //"compilerPath": "cl.exe",
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "intelliSenseMode": "windows-gcc-x64"
        },
        {
            "name": "Linux",
            "includePath": [
                "${default}",
                "${workspaceFolder}/**",
                "${workspaceFolder}/build/_deps/sfml-src/include"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
